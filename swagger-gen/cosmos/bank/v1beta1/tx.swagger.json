          "type": "array",
          "items": {
            "$ref": "#/definitions/cosmos.bank.v1beta1.SendEnabled"
          },
          "description": "send_enabled is the list of entries to add or update."
        },
        "use_default_for": {
          "type": "array",
          "items": {
            "type": "string"
          },
