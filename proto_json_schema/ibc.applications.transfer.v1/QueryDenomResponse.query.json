{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/QueryDenomResponse",
    "definitions": {
        "QueryDenomResponse": {
            "properties": {
                "denom": {
                    "$ref": "#/definitions/ibc.applications.transfer.v1.Denom",
                    "additionalProperties": true,
                    "description": "denom returns the requested denomination."
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Query Denom Response",
            "description": "QueryDenomResponse is the response type for the Query/Denom RPC method."
        },
        "ibc.applications.transfer.v1.Denom": {
            "properties": {
                "base": {
                    "type": "string",
                    "description": "the base token denomination"
                },
                "trace": {
                    "items": {
                        "$ref": "#/definitions/ibc.applications.transfer.v1.Hop"
                    },
                    "type": "array",
                    "description": "the trace of the token"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Denom",
            "description": "Denom holds the base denom of a Token and a trace of the chains it was sent through."
        },
        "ibc.applications.transfer.v1.Hop": {
            "properties": {
                "port_id": {
                    "type": "string"
                },
                "channel_id": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Hop",
            "description": "Hop defines a port ID, channel ID pair specifying a unique \"hop\" in a trace"
        }
    }
}