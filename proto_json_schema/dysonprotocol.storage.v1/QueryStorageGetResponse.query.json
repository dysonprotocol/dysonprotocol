{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/QueryStorageGetResponse",
    "definitions": {
        "QueryStorageGetResponse": {
            "properties": {
                "entry": {
                    "$ref": "#/definitions/dysonprotocol.storage.v1.Storage",
                    "additionalProperties": true
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Query Storage Get Response"
        },
        "dysonprotocol.storage.v1.Storage": {
            "properties": {
                "owner": {
                    "type": "string",
                    "description": "owner is the account address that owns this storage entry."
                },
                "index": {
                    "type": "string",
                    "description": "index is the key/index for the storage entry (within the owner's namespace)."
                },
                "data": {
                    "type": "string",
                    "description": "data is the stored string value associated with the index."
                },
                "updated_height": {
                    "type": "string",
                    "description": "updated_height is block height of last modification."
                },
                "updated_timestamp": {
                    "type": "string",
                    "description": "updated_timestamp is the block time in UTC (RFC3339 e.g. \"2025-06-11T15:23:00Z\") when entry was modified."
                },
                "hash": {
                    "type": "string",
                    "description": "hash is SHA-256 hex of data field."
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Storage"
        }
    }
}